public class B {
	
	
	
	boolean solve(int B, int M, char[][] conn) {
		//exact (n-2) * (n - 1) / 2 for n nodes
		
		if ((B-2) * (B-1) / 2  + 1 < M) return false;
		
		for(int i = 0; i < B - 1; i++) conn[i][i+1] = '1';
		
		int count = 1;
		
		for(int i = 0; i < B - 2; i++) {
			for(int j = i + 2; j < B; j++) {
				
			}
		}
	}
}
