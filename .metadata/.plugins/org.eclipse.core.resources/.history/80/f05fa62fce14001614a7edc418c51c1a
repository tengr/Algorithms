package GoogleCodeJam;

import java.util.Scanner;

//Problem 2016 1B Problem B
public class Match {
	public static void main(String[] args) {
        Match ins = new Match();
        Scanner sc = new Scanner(System.in);
        int testCase = sc.nextInt();
        for(int i = 1; i <= testCase; i++) {
        	String[] res = ins.solve(sc.next(), sc.next());
        	System.out.println("Case #" + i + ": " + res[0] + " " + res[1]);
        }
        sc.close();
    }
	public String[] solve(String a, String b){
		char[] coder = a.toCharArray();
		char[] jammer = b.toCharArray();
		char[] resCoder = new char[coder.length];
		char[] resJammer = new char[coder.length];
		//equal is one way
		int compare = 0;//equal
		for(int i = 0; i < coder.length; i++) {
			//both known
			if(coder[i] != '?' && jammer[i] != '?'){
				if(compare == 0) {
					if(coder[i] < jammer[i]) compare = -1; //coder < jammer
					else if(coder[i] > jammer[i]) compare = 1;
				}
			}//both unknown
			else if(coder[i] == '?' && jammer[i] == '?') {				
				if(compare == -1) { resCoder[i] = '9'; resJammer[i] = '1'; }
				else if(compare == 1) { resCoder[i] = '1'; resJammer[i] = '9';}
				else {resCoder[i] = '0'; resJammer[i] = '0';}
			}//one of them known
			else{
				if(compare == 0) {
					if(coder[i] == '?') coder[i] = jammer[i];
					else jammer[i] = coder[i];
				}
				else if(compare == -1) {
					if(coder[i] == '?') coder[i] = '9';
					else jammer[i] = '0';
				}
				else {
					if(coder[i] == '?') coder[i] = '0';
					else jammer[i] = '9';
				}
			}
				
		}
		String[] res = new String[2];
		res[0] = String.valueOf(coder);
		res[1] = String.valueOf(jammer);
		return res;	
	}
}
