package InterviewBitJava;

public class RepeatNumber {
	public int repeatedNumber(final List<Integer> a) {
	    int count = 0, res = 0;
	    for(Integer num : a) {
	        if(count == 0) {
	            res = num;
	        }
	        if(res == num) count += 2;
	        else count--;
	    }
	    count = 0;
	    for(Integer num : a) if(num == res) count++;
	    if(count > a.size() / 3) return res;
	    else return -1;
	}

}
