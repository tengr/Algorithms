
public class BST {
	BST left;
	BST right;
	int val;
	
	public BST(){
		left = null;
		right = null;
		val = Integer.MIN_VALUE;
	}
	
	public BST(int val){
		left = null;
		right = null;
		this.val = val;
	}
	
	public void inOrder(BST node) {
		if (node == null) return;
		inOrder(node.left);
		System.out.print(node.val + " ");
		inOrder(node.right);
	}
	
	public void preOrder(BST node){
		if(node == null) return;
		System.out.println(node.val + " ");
		preOrder(node.left);
		preOrder(node.right);
	}
	
	public static void main(String[] args){
		
		printBST
	}
}
