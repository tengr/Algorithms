package GeekForGeeks;

import java.util.Scanner;


public class CountDigitTest {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
	}
	public void input(Scanner sc) {
		int digit= -1, N = -1;
		do{
			System.out.println("digit = ");
			digit = sc.nextInt();
			System.out.println("range = ");
			N = sc.nextInt();
		}
		while(0 <= digit && digit <= 9 && N > 0);
		
		
		CountDigitInRange bruteforce = new CountDigitInRange(){
			@Override
			public int countinRange(int digit, int N) {
				if (N < 0) return 0;
				char c = (char)('0' + digit);
				String s = String.valueOf(N);
				for(int i = s.length() - 1; i >= 0; i--) {
					if(s.charAt(i) >= c) 
				}
				return 0;
			}
			
		};
		
		
		
		CountDigitInRange efficient = new CountDigitInRange(){
			@Override
			public int countinRange(int digit, int N) {
				int ret = 0;
				int count = 0;
				int preCount = 0;
				do {
					int d = N % 10;
					int pow = 1;
					if(d > digit) count = pow;
					else count = preCount;
					ret += count;
					preCount = count;
					N /= 10;
					pow *= 10;
				}while (N > 0);
				return ret;
			}
			
		};
		
	}
}
